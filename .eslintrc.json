{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "react/jsx-filename-extension": ["off"],
        "react/react-in-jsx-scope": ["off"],
        "quotes": ["error", "double"]
    }
}
